{
  "version": 2,
  "buildCommand": "npm run build && cd .medusa/server && npm install && npm run predeploy",
  "outputDirectory": ".medusa",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/app/assets/(.*)",
      "dest": "/admin/assets/$1"
    },
    {
      "src": "/app",
      "dest": "/admin/index.html"
    },
    {
      "src": "/app/(.*)",
      "dest": "/admin/index.html"
    },
    {
      "src": "/store/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
